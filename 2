stage('Build the Dcoker images'){
    steps{
        sh' sudo docker build -t newimage /var/lib/jenkins/workspace/bala pipeline'
        sh 'sudo docker tag newimage balasubramaniyand/newimages:lastest'
        sh 'sudo docker tag  newimage balasubramaniyand/newimage:${BUILD_NUMBER}'
    }
    }
    
