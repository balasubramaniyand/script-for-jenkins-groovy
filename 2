pipeline {
    agent any
    stages {
        stage ( 'Pull Code From Github'){
        steps{
           git 'https://github.com/balasubramaniyand/k8sproject.git'
        }
    }
stage('docker'){
    steps {
        sh' docker images'
    }
}
stage('Build the Dcoker images'){
    steps{
        sh' sudo docker build -t balasubramaniyand/love:latest /var/lib/jenkins/workspace/love'
        sh 'sudo docker tag  balasubramaniyand/love:latest balasubramaniyand/love:${BUILD_NUMBER}'
    }
    }
    
}
}
